
# Powerline Daemon
# export PATH="$HOME/.local/bin:$PATH"
powerline-daemon -q

# Powerline for Zsh
if [ -f /usr/local/lib/python2.7/dist-packages/powerline/bindings/zsh/powerline.zsh ]; then
      . /usr/local/lib/python2.7/dist-packages/powerline/bindings/zsh/powerline.zsh
fi

