# Custom Aliases
if [ -f ~/.custom_aliases ]; then
      . ~/.custom_aliases
fi

# tmuxinator
if [ -f ~/.bin/tmuxinator.zsh ]; then
      . ~/.bin/tmuxinator.zsh
fi

