execute pathogen#infect()
set t_Co=256
set rtp+=~/.vim/bundle/powerline/powerline/bindings/vim
syntax on
filetype plugin indent on
set cursorline
set number
set autoindent
set fileencoding=utf-8
set encoding=utf-8
set background=dark
set laststatus=2
map <F2> :NERDTreeToggle<CR>
colorscheme xoria256
nnoremap <silent> <F3> :set paste!<CR>
set pastetoggle=<F3>
