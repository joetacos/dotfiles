
# Powerline Daemon
export PATH="$HOME/.local/bin:$PATH"
powerline-daemon -q

# Powerline for Bash
if [ -f /usr/local/lib/python2.7/dist-packages/powerline/bindings/bash/powerline.sh ]; then
      . /usr/local/lib/python2.7/dist-packages/powerline/bindings/bash/powerline.sh
fi

