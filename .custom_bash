#       . ~/.local/lib/python2.7/site-packages/powerline/bindings/bash/powerline.sh
# fi

# Custom Aliases
if [ -f ~/.custom_aliases ]; then
      . ~/.custom_aliases
fi

# tmuxinator
if [ -f ~/.bin/tmuxinator.bash ]; then
      . ~/.bin/tmuxinator.bash
fi
